generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model WikiHistoricalEvent {
  id            Int     @id @default(autoincrement())
  year          Int
  month         Int
  day           Int
  event         String
  thumbnail_url String?
}
