datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model HistoricalEvent {
  id    Int     @id @default(autoincrement())
  year  Int
  month Int
  day   Int
  event String
}

model WikiHistoricalEvent {
  id            Int     @id @default(autoincrement())
  year          Int
  month         Int
  day           Int
  event         String
  thumbnail_url String?
}